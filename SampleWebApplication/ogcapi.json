{
  "LandingPage": {
    "Title": "OGC API Sample Application",
    "Description": "OGC API Sample Application",
    "ApiDocumentPage": "https://localhost:5001/index.html",
    "ApiDescriptionPage": "https://localhost:5001/api/ogc/swagger.json"
  },
  "UseApiKeyAuthorization": true,
  "Collections": {
    "Items": [
      {
        "Id": "Polygons",
        "Title": "Polygons",
        "Features": {
          "Crs": [
            "http://www.opengis.net/def/crs/OGC/1.3/CRS84",
            "http://www.opengis.net/def/crs/EPSG/0/3857"
          ],
          "StorageCrs": "http://www.opengis.net/def/crs/EPSG/0/3857",
          "Storage": {
            "Type": "SqlServer",
            "ConnectionString": "Server=localhost;Integrated Security=true;Initial Catalog=test",
            "Schema": "dbo",
            "Table": "Polygons",
            "GeometryColumn": "Geom",
            "GeometryDataType": "geometry",
            "GeometryGeoJsonType": "Polygon",
            "GeometrySrid": 3857,
            "DateTimeColumn": "Date",
            "IdentifierColumn": "Id",
            "Properties": [
              "Name",
              "Number",
              "S",
              "Date"
            ],
            "AllowCreate": true,
            "AllowReplace": true,
            "AllowUpdate": true,
            "AllowDelete": true
          }
        },
        "Tiles": {
          "Crs": "http://www.opengis.net/def/crs/EPSG/0/3857",
          "Storage": {
            "Type": "MbTiles",
            "FileName": "Data\\data.mbtiles"
          }
        }
      }
    ]
  }
}